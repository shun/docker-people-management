{
  "name": "app",
  "version": "0.0.1",
  "description": "people management backend service.",
  "devDependencies": {
    "@types/bcrypt": "^10.0.28-alpha",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/eslint": "^7.2.11",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/mysql": "^2.15.18",
    "@types/node": "^8.0.29",
    "@types/nodemon": "^1.19.0",
    "@types/passport": "^0.4.7",
    "@types/prettier": "^2.2.3",
    "@types/supertest": "^2.0.11",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-server-express": "^2.24.1",
    "apollo-server-testing": "^2.24.1",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.2",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.5",
    "jest": "^26.6.3",
    "keycloak-connect": "^13.0.0",
    "mysql": "^2.14.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "openid-client": "^4.7.3",
    "passport": "^0.4.1",
    "prettier": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "tsconfig-paths": "^3.9.0",
    "type-graphql": "^1.1.1",
    "typeorm": "0.2.32"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "jest",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop",
    "migration:init": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n Init",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:show": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:show",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "lint": "eslint \"**/*.ts\"",
    "format": "prettier --write \"**/*.{js,ts,json,md}\""
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.spec.ts"
    ]
  }
}
